            <ITEXT CH="AlternC"/>
            <para PARENT="h1"/>
            <ITEXT CH="Le panel d'administration web AlternC utilisae MPM-ITK depuis sa version 3.1 "/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="AlternC users and files"/>
            <para PARENT="h2"/>
            <ITEXT CH="Avec AlternC les identifiants UserId de MPM-ITK sont égaux à ceux des logins AlternC : les fichiers et dossiers sont crées en leur attribuant des droits correspondant à cette identité."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Fichiers et ACL étendus"/>
            <para PARENT="h2"/>
            <ITEXT CH="AlternC dépend aussi des &quot;Extended Access Control List&quot; pour définir plus d'un utilisateur / groupe propriétaire des fichiers et dossiers.  "/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$"/>
            <nbspace/>
            <ITEXT CH="getfacl /var/www/the-domain-name.com/index.php"/>
            <para PARENT="p.code"/>
            <ITEXT CH="# file: var/www/the-domain-name.com/"/>
            <para PARENT="p.code"/>
            <ITEXT CH="# owner: 2001"/>
            <para PARENT="p.code"/>
            <ITEXT CH="# group: 2001"/>
            <para PARENT="p.code"/>
            <ITEXT CH="# flags: -s-"/>
            <para PARENT="p.code"/>
            <ITEXT CH="user::rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="user:alterncpanel:rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="user:2001:rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="group::rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="group:alterncpanel:rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="group:2001:rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="mask::rwx"/>
            <para PARENT="p.code"/>
            <ITEXT CH="other::---"/>
            <para PARENT="p.code"/>
            <ITEXT CH="Chaque fichier ou dossier appartient à l'utilisateur AlternC et également à l'utilisateur alterncpanel, afin que le panel AlternC puisse interagir avec les fichiers et les dossiers du dossier et notamment éditer des fichiers avec l'éditeur de fichier AlternC."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="L'utilitaire fixperms.sh "/>
            <para PARENT="h2"/>
            <ITEXT CH="En éditant avec un compte UNIX non-AlternC (ex : root) des fichiers , leur propriétaire par défaut est ce compte UNIX, ce qui peut causer des bugs en raison de droits invalides pour MPM-ITK. "/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="L'utilitaire fixperms.sh répare la correspondance entre les ACL étendues des fichiers / dossiers et les comptes AlternC dont ils dépendent."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Location"/>
            <para PARENT="h4"/>
            <ITEXT CH="/usr/lib/alternc/fixperms.sh"/>
            <para PARENT="p.code"/>
            <ITEXT CH="Usage"/>
            <para PARENT="h4"/>
            <ITEXT CH="fixperms "/>
            <tab/>
            <para PARENT="p.code"/>
            <tab/>
            <ITEXT CH="Répare toutes les autorisations pour tous les comptes "/>
            <para PARENT="notes"/>
            <ITEXT CH="fixperms -d /var/alternc/t/the_user/"/>
            <para PARENT="p.code"/>
            <tab/>
            <ITEXT CH="Répare un [d]ossier unique"/>
            <para PARENT="notes"/>
            <ITEXT CH="fixperms -f /var/alternc/t/the_user/www/index.php"/>
            <para PARENT="p.code"/>
            <tab/>
            <ITEXT CH="Répare un [f]ichier unique"/>
            <para PARENT="notes"/>
            <ITEXT CH="fixperms -l the_user"/>
            <para PARENT="p.code"/>
            <tab/>
            <ITEXT CH="Répare le dossier d'un [l]ogin AlternC"/>
            <para PARENT="notes"/>
            <ITEXT CH="fixperms -u 2001"/>
            <para PARENT="p.code"/>
            <tab/>
            <ITEXT CH="Répare le dossier d'un login AlternC par [u]id"/>
            <para PARENT="notes"/>
            <ITEXT CH="AlternC-SSH"/>
            <para PARENT="h2"/>
            <ITEXT CH="AlternC-SSH est un module AlternC qui génère un compte UNIX réel pour chaque compte AlternC."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="L'administrateur peut ajouter des clefs SSH à la base de données AlternC. Soit une clef &quot;admin&quot; valable pour chaque compte, soit une clef &quot;utilisateur&quot; valable pour un compte unique."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="À la création d'un nouveau login AlternC, le module AlternC-SSH crée le dossier &quot;home&quot; de l'utilisateur  et le jeu de clefs SSH autorisées."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Directives de configuration MPM-ITK "/>
            <para PARENT="h1"/>
            <ITEXT CH="AssignUserID"/>
            <para PARENT="h3"/>
            <ITEXT CH="Prend 2 paramètres, uid et gid, qui assignent le uid et gid sous lesquels le vhost tourne. On utilise soit les uid numériques sous la forme #&lt;int&gt; soit les noms des utilisateurs et groupes. Notez que sans user ID, ce sont les défauts d'Apache qui seront utilisés."/>
            <para/>
            <ITEXT CH="MaxClientsVHost"/>
            <para PARENT="h3"/>
            <ITEXT CH="Un nombre maximal de client par vhost qui permet d'empêcher qu'un vhost unique préempte la totalité du nombre maximal de clients du serveur."/>
            <para/>
            <ITEXT CH="NiceValue"/>
            <para PARENT="h3"/>
            <ITEXT CH="Permet de réduire la priorité d'exécution des requêtes sur le vhost pour leur donner moins de temps CPU."/>
            <para/>
            <ITEXT CH="AssignUserIDExpr, AssignGroupIDExpr "/>
            <para PARENT="h3"/>
            <ITEXT CH="(Apache 2.4 ou plus récent)"/>
            <para PARENT="notes"/>
            <ITEXT CH="Comme AssignUserID, mais utilise une expression régulière Apache pour choisur un uid ou un groupe."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="LimitUIDRange, LimitGIDRange "/>
            <para PARENT="h3"/>
            <ITEXT CH="(Apache 2.4 ou plus récent)"/>
            <para PARENT="notes"/>
            <ITEXT CH="Restreint les appels à setuid() and setgid() à une plage pour empêcher la création de vhosts en dehors avec des uid bas notamment. "/>
            <para/>
            <ITEXT CH="Sources"/>
            <para PARENT="h1"/>
            <ITEXT CH="http://mpm-itk.sesse.net/"/>
            <para/>
            <ITEXT CH="http://agix.com.au/blog/?p=2124"/>
            <para/>
