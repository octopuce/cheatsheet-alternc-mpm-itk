            <ITEXT CH="AlternC"/>
            <para PARENT="h1"/>
            <ITEXT CH="Le panel d'administration web AlternC utilisae MPM-ITK depuis sa version 3.1 "/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="AlternC users and files"/>
            <para PARENT="h2"/>
            <ITEXT CH="Avec AlternC les identifiants UserId de MPM-ITK sont égaux à ceux des logins AlternC : les fichiers et dossiers sont crées en leur attribuant des droits correspondant à cette identité."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Fichiers et ACL étendus"/>
            <para PARENT="h2"/>
            <ITEXT CH="AlternC dépend aussi des &quot;Extended Access Control List&quot; pour définir plus d'un utilisateur / groupe propriétaire des fichiers et dossiers.  "/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="$"/>
