            <ITEXT CH="Ajouter des fichiers à un serveur AlternC MPM-ITK "/>
            <para PARENT="h2"/>
            <ITEXT CH="Les options sont présentées par ordre de préférence décroissante."/>
            <para/>
            <ITEXT CH="#1 Utiliser l'éditeur de fichiers ou un compte FTP AlternC"/>
            <para PARENT="h3"/>
            <ITEXT CH="Ce sont les solutions parfaites pour créer un simple fichier ou dossier."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="#2 Utiliser le module AlternC-SSH"/>
            <para PARENT="h3"/>
            <ITEXT CH="En se loggant en SSH avec l'utilisateur"/>
            <para PARENT="notes"/>
            <ITEXT CH="Utiliser le login du compte AlternC"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CPARENT="Default Character Style" CH="Ex:"/>
            <tab CPARENT="Default Character Style"/>
            <ITEXT CPARENT="Default Character Style" CH="me@laptop$ ssh the_user@myserver.com"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Ou en changeant de personnalité avec su"/>
            <para PARENT="notes"/>
            <ITEXT CH="Une fois loggé en tant que root, changer d'identité "/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CPARENT="Default Character Style" CH="Ex:"/>
            <tab CPARENT="Default Character Style"/>
            <ITEXT CPARENT="Default Character Style" CH="root@myserver# su the_user"/>
            <para PARENT="p.code"/>
            <ITEXT CH="#3 Utiliser le module AlternC-LXC"/>
            <para PARENT="h3"/>
            <ITEXT CPARENT="Default Character Style" CH="Demander une"/>
            <ITEXT CH=" nouvelle instance LXC et se logger en SSH. Tous les fichiers sont créés / édités avec les droits du compte AlternC."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="#4 Corriger avec fixperms.sh"/>
            <para PARENT="h3"/>
            <ITEXT CH="Se logger avec le compte Unix root, éditer/ajouter des fichiers et remettre les droits d'aplomb avec l'utilitaire."/>
            <trail PARENT="Default Paragraph Style"/>
