            <ITEXT CH="MPM-ITK"/>
            <para PARENT="h1"/>
            <ITEXT CH="Le module MPM-TK fonctionne comme le module classique &quot;prefork&quot; -autrement dit sans threads- à l'exception du fait qu'il permet de faire tourner chaque vhost sous un utilisateur spécifique."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Plusieurs sites web peuvent ainsi être disponibles sur un serveur unique sans avoir à se soucier du fait qu'un utilisateur puisse lire les fichiers d'un autre."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Si avec un serveur classique protégé via les directives de sécurité PHP les fichiers étaient possédés par l'utilisateur www-data, désormais les fichiers des sites doivent être possédés par des utilisateurs différents."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Virtual hosts"/>
            <para PARENT="h2"/>
            <ITEXT CH="Avec MPM-ITK, chaque vhost Apache est assigné à un utilisateur."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="&lt;Virtualhost *:80&gt;"/>
            <para PARENT="p.code"/>
            <ITEXT CH="  ServerName www.the-domain-name.com"/>
            <para PARENT="p.code"/>
            <ITEXT CH="  ServerRoot /var/www/the-domain-name.com"/>
            <para PARENT="p.code"/>
            <ITEXT CH="  AssignUserId #2001 #2001 "/>
            <para PARENT="p.code"/>
            <ITEXT CH="  SetEnv LOGIN &quot;2001-the_user&quot;"/>
            <para PARENT="p.code"/>
            <ITEXT CH="&lt;/Virtualhost&gt;"/>
            <para PARENT="p.code"/>
            <ITEXT CH="File system "/>
            <para PARENT="h2"/>
            <ITEXT CH="L'utilisateur doit disposer de droits suffisants sur les fichiers de ce vhost."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="# ls -lh  /var/www/the-domain-name.com"/>
            <para PARENT="p.code"/>
            <ITEXT CH="-rwxrws---+  1 2001 2001   418 sept. 25  2013 index.php"/>
            <para PARENT="p.code"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
