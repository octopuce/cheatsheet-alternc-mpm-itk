            <ITEXT CH="# owner: 2001"/>
            <para PARENT="p.code"/>
            <ITEXT CH="# group: 2001"/>
            <para PARENT="p.code"/>
            <ITEXT CH="# flags: -s-"/>
            <para PARENT="p.code"/>
            <ITEXT CH="user::rwx"/>
